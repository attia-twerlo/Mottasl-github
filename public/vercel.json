{
  "rewrites": [
    { "source": "/assets/(.*)", "destination": "/assets/$1" },
    { "source": "/favicon.ico", "destination": "/favicon.ico" },
    { "source": "/Logo.svg", "destination": "/Logo.svg" },
    { "source": "/file.svg", "destination": "/file.svg" },
    { "source": "/globe.svg", "destination": "/globe.svg" },
    { "source": "/window.svg", "destination": "/window.svg" },
    { "source": "/icons/(.*)", "destination": "/icons/$1" },
    { "source": "/logos/(.*)", "destination": "/logos/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}